    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#verloningsperiode",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "definition": {
        "nl": "Een bepaalde tijdsduur met een start- en eindmoment die dient als referentie voor de bepaling van het brutoloon."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/loongegevens#Arbeidsloon"
