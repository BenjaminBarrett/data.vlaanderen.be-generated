      ],
      "generalization": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#vakantiegeld",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "definition": {
        "nl": "Dit is een jaarlijkse toelage die een werknemer krijgt om de extra kosten van vakantie te dekken."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/loongegevens#Arbeidsloon"
      ],
      "range": [
        "https://schema.org/MonetaryAmount"
      ],
      "generalization": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#tewerkstellingsbreuk",
