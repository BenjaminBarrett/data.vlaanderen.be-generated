    },
    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#bankrekening",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "definition": {
        "nl": "Een rekeningnummer dat gelinkt is aan de arbeidsrelatie. Op dit rekeningnummer wordt het arbeidsloon van de persoon gestort."
      },
