  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#Arbeidsloon",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Het bedrag dat een persoon ontvangt voor geleverde prestaties in arbeidsverband. Het kan gaan om een financieel bedrag, een bedrag in natura, vergoedingen, etc."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/loongegevens#Inkomst"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#Arbeidsrelatie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "De betrekking tussen werkgevers en werknemers die ontstaat bij loonarbeid na afsluiting van een arbeidsovereenkomst."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#Inkomst",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Het bedrag dat een persoon ontvangt voor geleverde prestaties, beschikbaar gestelde goederen, land of vermogen."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/loongegevens#Tewerkstellingsbreuk",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Het gemiddeld aantal uren per week waarin de werknemer geacht wordt te werken op basis van zijn contract, gedeeld door het gemiddeld aantal uren per week waarin een modelpersoon geacht wordt te werken."
      },
      "parents": []
    }
  ],
