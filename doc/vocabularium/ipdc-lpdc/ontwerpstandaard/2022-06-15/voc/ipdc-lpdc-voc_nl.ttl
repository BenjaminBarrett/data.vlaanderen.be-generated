  rdfs:isDefinedBy <http://fixme.com>;
  rdfs:range <http://www.w3.org/ns/locn#Address> .

<http://fixme.com#country> a owl:ObjectProperty;
  <http://purl.org/vocab/vann/usageNote> "Omwille van de meertaligheid kunnen er meerdere waarden zijn. De gewenste interpretatie van de kardinaliteit is: per taal slechts 1 waarde."@nl;
  rdfs:domain <http://www.w3.org/ns/locn#Address>;
  rdfs:isDefinedBy <http://fixme.com>;
  rdfs:range rdf:langString;
  rdfs:subPropertyOf <http://www.w3.org/ns/locn#adminUnitL1> .

<http://fixme.com#endDate> a owl:DatatypeProperty;
  rdfs:comment "Datum waarop de publieke dienstverlening stopgezet werd/wordt"@nl;
  rdfs:domain <http://purl.org/vocab/cpsv#PublicService>;
  rdfs:isDefinedBy <http://fixme.com>;
  rdfs:range xsd:dateTime .

<http://fixme.com#exceptions> a owl:ObjectProperty;
  <http://purl.org/vocab/vann/usageNote> "Omwille van de meertaligheid kunnen er meerdere waarden zijn. De gewenste interpretatie van de kardinaliteit is: per taal slechts 1 waarde."@nl;
